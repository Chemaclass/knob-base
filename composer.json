	"keywords": ["knob","mvc","wordpress"],
	"homepage": "http://knob.chemaclass.com",
	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "José María Valera Reales",
			"email": "chemaclass@outlook.es",
			"homepage": "http://chemaclass.com"
		}
	],
	"require": {
		"php": ">=5.6",
		"phpmailer/phpmailer": "5.2.*"
	},
	"require-dev": {
		"phpunit/phpunit": "^6.0"
	},
	"autoload": {
		"psr-0": {
